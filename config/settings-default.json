{
	"apiRoot": "/api",
	"cryptoKey": "***CHANGE THIS***",
	"sandboxes": {
		"ttl": 300000
	},
	"siteEmail": {
		"address": "admin@example.com",
		"name": "Site administrator"
	},
	"siteName": "Shrub example application",
	"packageList": [
		"angular",
		"assets",
		"config",
		"core",
		"example",
		"express",
		"files",
		"form",
		"http",
		"limiter",
		"logger",
		"nodemailer",
		"repl",
		"rpc",
		"schema",
		"session",
		"socket",
		"socket.io",
		"ui",
		"user",
		"villiany"
	],
	"packageSettings": {
		"angular": {
			"navigationMiddleware": [
				"form"
			],
			"ttl": 300000
		},
		"core": {
			"trustedProxies": []
		},
		"http": {
			"manager": {
				"module": "packages/express"
			},
			"middleware": [
				"core",
				"socket/factory",
				"form",
				"session/express",
				"session",
				"user",
				"express/logger",
				"express/routes",
				"express/static",
				"config",
				"assets",
				"angular",
				"express/errors"
			],
			"port": 4201
		},
		"nodemailer": {
			"transport": {
				"type": "sendmail",
				"options": {}
			}
		},
		"repl": {
			"prompt": "shrub> ",
			"useCoffee": true
		},
		"schema": {
			"corsHeaders": null
		},
		"session": {
			"sessionStore": "redis",
			"key": "connect.sid",
			"cookie": {
				"cryptoKey": "***CHANGE THIS***",
				"maxAge": 1209600000
			}
		},
		"socket": {
			"authorizationMiddleware": [
				"core",
				"session/express",
				"user",
				"villiany"
			],
			"connectionMiddleware": [
				"session",
				"user",
				"rpc"
			],
			"disconnectionMiddleware": [],
			"manager": {
				"module": "packages/socket.io"
			}
		},
		"user": {
			"beforeLoginMiddleware": [],
			"afterLoginMiddleware": [],
			"beforeLogoutMiddleware": [
				"user"
			],
			"afterLogoutMiddleware": [
				"user"
			]
		},
		"villiany": {
			"ban": {
				"defaultExpiration": 600000
			}
		}
	}
}
