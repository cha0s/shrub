{
	"except": [
		"client/**/*.spec.coffee",
		"client/**/test-e2e.coffee",
		"client/**/test-unit.coffee",
		"server/**/*.spec.coffee",
		"server/**/test-e2e.coffee",
		"server/**/test-unit.coffee"
	],
	"glob": [
		"README.md",
		"documentation/**/*.md",
		"server.coffee",
		"client/**/*.coffee",
		"server/**/*.coffee",
		"custom/**/*.coffee",
		"packages/**/*.coffee"
	],
	"index": "./documentation/index.md",
	"index-page-title": "Shrub documentation",
	"out": "./gh-pages",
	"repository-url": "https://github.com/cha0s/shrub",
	"strip": [
		"documentation"
	],
	"style": "shrub-groc-style"
}
